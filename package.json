{
  "name": "china-musk",
  "version": "0.7.7",
  "private": true,
  "description": "an app that will send a file to Twitter, one line per tweet",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.3.2",
    "classnames": "^2.2.6",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "voca": "^1.4.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2",
    "react-scripts": "^3.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "homepage": "https://tweries.com",
  "proxy": "http://localhost:9000"
}
